{
  "name": "highlit-server",
  "version": "1.0.0",
  "description": "The backend server for Highlit",
  "main": "build/app.js",
  "types": "build/app.d.ts",
  "scripts": {
    "serve": "env NODE_ENV=test nodemon",
    "start:test": "env NODE_ENV=test node --inspect build/app.js",
    "start:prod": "pm2 start build/app.js",
    "build": "rimraf ./build && tsc",
    "test": "ts-node src/Tests/TestManager.ts",
    "db:build": "sqlite3 -init db/init_db.sql db/matches.db \"\"",
    "db:clear": "sqlite3 db/matches.db < db/reset_db.sql"
  },
  "author": "Minifixio",
  "license": "ISC",
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/node": "^14.0.5",
    "@types/nodemailer": "^6.4.0",
    "@types/request": "^2.48.5",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/bytebuffer": "^5.0.41",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.154",
    "@types/rimraf": "^3.0.0",
    "@types/sqlite3": "^3.1.6",
    "bytebuffer": "^5.0.1",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "demofile": "^1.3.1",
    "express": "^4.17.1",
    "hltv": "^2.19.4",
    "lodash": "^4.17.19",
    "node-unrar-js": "^0.8.1",
    "nodemailer": "^6.4.6",
    "socket.io": "^2.3.0",
    "sqlite3": "^4.2.0",
    "twitch-video-comments-analyser": "^1.0.4",
    "unrar": "^0.2.0",
    "unrar-promise": "^2.0.1",
    "unzipper": "^0.10.11",
    "winston": "^3.2.1"
  }
}
